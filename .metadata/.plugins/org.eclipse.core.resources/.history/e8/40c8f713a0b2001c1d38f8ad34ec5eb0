package com.harman.ui;

import com.harman.model.Employee;
import com.harman.model.Person;

import com.harman.model.StudentE;

public class InheritanceApp {

	public static void main(String[] args) {
		Person p1=new Person("Riya",21);
		System.out.println(p1.getName()+" "+p1.getAge());
		
		StudentE p2=new StudentE("Johnson",25,3000.0);
		System.out.println(p2.getName()+" "+p2.getAge()+" "+p2.getfee());

			
		Employee E1=new Employee("Savin",28,50000.0);
		System.out.println(E1.getName()+" "+E1.getage()+" "+E1.getSalary());
	}

}
